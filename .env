# Example environment for EduTrack
# Copy to .env and fill values as needed

# App
APP_ENV=development
APP_DEBUG=1
APP_URL=http://localhost/edutrack/

# Database (preferred keys)
DB_HOST=127.0.0.1
DB_NAME=edutrack_db
DB_USER=root
DB_PASS=

# SMTP (used by PHPMailer)
SMTP_HOST=smtp.mailtrap.io
SMTP_PORT=2525
SMTP_ENCRYPTION=tls
SMTP_USERNAME=
SMTP_PASSWORD=
SMTP_FROM=support@edutrack.local
SMTP_FROM_NAME="EduTrack Support"
SUPPORT_TO=you@example.com
